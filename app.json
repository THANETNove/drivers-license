{
  "expo": {
    "name": "driversLicense",
    "slug": "driversLicense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coverImg/Artboard90.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/coverImg/3_0.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coverImg/Artboard90.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ththanet.driversLicense"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d0e4c602-55f1-4b12-b923-52775fc91246"
      }
    }
  }
}
